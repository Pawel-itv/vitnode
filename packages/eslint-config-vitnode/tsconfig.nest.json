{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.shared.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "./dist",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "noFallthroughCasesInSwitch": false
  }
}
